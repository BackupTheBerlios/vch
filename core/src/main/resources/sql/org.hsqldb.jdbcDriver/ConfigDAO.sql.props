SQL_FIND_BY_KEY = SELECT * FROM {TABLE_NAME} WHERE PARAMETERKEY = ?

SQL_INSERT_CONFIG = INSERT INTO {TABLE_NAME} \
                           (PARAMETERKEY, PARAMETERVALUE) \
                           values(?,?)

SQL_UPDATE_CONFIG = UPDATE {TABLE_NAME} \
                       SET PARAMETERVALUE = ? \
                       WHERE PARAMETERKEY = ?
    
SQL_DELETE_CONFIG = DELETE FROM {TABLE_NAME} WHERE PARAMETERKEY = ?

SQL_LIST_ALL = SELECT * FROM {TABLE_NAME}