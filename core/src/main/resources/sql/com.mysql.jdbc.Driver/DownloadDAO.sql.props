SQL_FIND_BY_KEY = SELECT * FROM {TABLE_NAME} WHERE ITEMKEY = ?

SQL_INSERT_DOWNLOAD = INSERT INTO {TABLE_NAME} \
                           (ITEMKEY, LOCALFILE) \
                           values(?,?)

SQL_UPDATE_DOWNLOAD = UPDATE {TABLE_NAME} \
                       SET LOCALFILE = ? \
                       WHERE ITEMKEY = ?
    
SQL_DELETE_DOWNLOAD = DELETE FROM {TABLE_NAME} WHERE ITEMKEY = ?

SQL_LIST_ALL = SELECT * FROM {TABLE_NAME}